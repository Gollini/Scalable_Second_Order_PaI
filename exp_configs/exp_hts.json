{
    "general": {
        "id": "exp_hts",
        "description": "resnet18 hts metric, mask batch size 1, warmup 0, sparsity 0.9, seed 0, structured False, distributed False",
        "gpu": {
            "ngpus": 1,
            "gpu_id": 0
        },
        "experiment": "pbt",
        "seed": 0,
        "output_dir": "./outputs"
    },
    "dataset": {
        "class": "cifar10",
        "batch_size": 512,
        "num_workers": 2,
        "train4val": false
    },
    "model": {
        "class": "resnet18",
        "num_classes": 10
    },
    "training": {
        "num_steps": 5,
        "local_epochs": 0,
        "criterion": {
            "class": "CE",
            "kwargs": {}
        },
        "optimizer": {
            "class": "sgd",
            "learning_rate": 0.03,
            "kwargs": {
                "momentum": 0.9,
                "w_decay": 0.0005,
                "scheduler": "step",
                "lr_drops": [
                    80,
                    120
                ],
                "lr_drop_factor": 0.2
            }
        },
        "compressor": {
            "class": "hts",
            "mask": "global",
            "sparsity": 0.9,
            "warmup": 0,
            "batch_size": 1,
            "per_class_samples": 10
        }
    },
    "validation": {
        "metric": "acc",
        "frequency": 1
    }
}